<body>
<!-- MDB -->
<main>


    <div class="container mt-4" id="my_nanogallery2"
         data-nanogallery2 = '{
                        "thumbnailHeight":  auto,
                        "thumbnailWidth":   50,
                     '>
        <% if(photos.length > 0){ %>
            <% photos.forEach(function(media) { %>
                <a href="<%= media.url%>" data-ngthumb="/images/placeholder.png"><%= media.name%></a>

            <%

            }); %>
        <% } else{ %>
            <h1 class="text-center"> No photo found for this user</h1>
        <% } %>

    </div>
    <div class="nanogallery">










    </div>
    <div id="nanogallery"></div>
</main>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script
